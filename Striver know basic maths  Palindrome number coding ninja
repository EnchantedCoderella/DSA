import java.util.Scanner;
public class Solution {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int b=n,a=0,sum=0;
		while(b>0){
			a=b%10;
			sum=sum*10+a;
			b/=10;
		}
		if(sum==n){
			System.out.print("true");
		} else{
			System.out.print("false");
		}
	}
}
Output:- 
